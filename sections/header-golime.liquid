              class="header__icon sub-golime-cart-icon header__icon--cart link focus-inset"
              id="cart-icon-bubble"
            >
              {% if cart == empty %}
                {% if section.settings['cart-icon'] != blank %}
                  <img
                    src="{{ section.settings.cart-icon |img_url:'master' }}"
                    alt="{{ section.settings.cart-icon.alt | escape }}"
                    class="main-sub-icon-golime-cart-icon-images-img"
