<style>
  .G_O_banner_section_Golime_new_banner {
      padding-bottom: 30px;
      color: white;
  }

  .G_O_banner_section_innr {
      position: relative;
  }

  .G_O_banner_section_images_box {
      position: absolute;
      width: 100%;
      top: 0;
      height: 100%;
  }

  .G_O_banner_section_img_desktop {
      position: absolute;
      top: 0;
      right: 0;
      left: 0;
      bottom: 0;
      object-fit: cover;
      width: 100%;
      height: 100%;
      object-position: right bottom;
  }

  .G_O_banner_section_img_mobile {
      display: none;
  }

  .G_O_banner_section_position_bar {
      position: relative;
      padding: 110px 40px 0;
      width: 100%;
  }

  .G_O_banner_section_position_bar_container {
      max-width: 1437px;
      margin: 0 auto;
      width: 100%;
      padding: 12px;
  }

  .G_O_banner_section_position_bar_content {
      max-width: 690px;
  }

  .G_O_banner_section_position_bar_Heading {
      padding: 20px 0 10px;
      line-height: 56px;
      font-size: 56px;
      font-weight: 800;
      color: white;
  }

  .G_O_banner_section_position_bar_Para p {
      font-size: 20px;
      margin: 0;
      line-height: normal;
      color: white;
  }

  .G_O_banner_section_position_bar_button_text {
      padding-top: 24px;
      display: flex
  ;
      align-items: center;
      gap: 24px;
  }

  .G_O_banner_section_position_button_by {
      text-decoration: none;
      color: white;
      font-size: 20px;
      background: #085A54;
      display: flex
  ;
      width: fit-content;
      white-space: nowrap;
      padding: 0 26px;
      border-radius: 50px;
      align-items: center;
      gap: 17px;
      letter-spacing: normal;
      height: 52px;
      transition: 0.8s all ease-in-out;
      cursor: pointer;
  }

  .fa-solid.fa-chevron-right {
      font-size: 14px;
  }

  .G_O_banner_section_position_Text {
      text-decoration: none;
      color: white;
      display: flex
  ;
      align-items: center;
      gap: 10px;
      letter-spacing: 0;
      line-height: normal;
      font-size: 20px;
      cursor: pointer;
      font-family: 'Avenir_LT_Std_65_Medium' !important;
  }

  .G_O_banner_section_position_text {
      position: relative;
      text-decoration: none;
      display: inline-block;
      cursor: pointer;
      font-size: 20px;
      font-family: 'Avenir_LT_Std_65_Medium' !important;
  }

  .G_O_banner_section_position_text::after {
      content: "";
      position: absolute;
      left: 0;
      bottom: 0;
      height: 1px;
      width: 0;
      background-color: currentColor;
      transition: width 0.3s ease-in-out;
  }

  .G_O_banner_section_position_text:hover::after {
      width: 100%;
  }

  .G_O_banner_section_position_text_btm_images {
      position: relative;
      height: 420px;
      width: 68%;
      margin-top: 20px;
  }

  .G_O_banner_section_position_text_btm_img {
      width: 100%;
      height: 100%;
      position: absolute;
      top: 0;
      right: 0;
      left: 0;
      border: 0;
      object-fit: contain;
      object-position: left;
      padding-bottom: 30px;
      bottom: 75px;
  }

  @media screen and (max-width: 767px) {
      .G_O_banner_section_Golime_new_banner {
          padding-bottom: 90px;
      }
      .G_O_banner_section_img_desktop {
      display: none;
  }

  img.G_O_banner_section_img_mobile {
      display: block;
      position: absolute;
      top: 0;
      right: 0;
      left: 0;
      bottom: 0;
      object-fit: cover;
      width: 100%;
      height: 100%;
      object-position: bottom center;
      filter: blur(5px);
      mask-image: linear-gradient(to bottom, black 80%, transparent 70%);
  }

  .G_O_banner_section_position_bar {
      padding: 120px 15px 0;
  }

  .G_O_banner_section_position_bar_container {
      padding: 0;
  }

  .G_O_banner_section_position_bar_content {
      text-align: center;
      margin: 0 auto;
  }

  .G_O_banner_section_position_bar_Heading {
      padding-top: 0px;
      padding-bottom: 10px;
      line-height: 36px;
      font-size: 32px;
  }

  .G_O_banner_section_position_bar_Para p {
      font-size: 18px;
      padding: 0px;
  }

  .G_O_banner_section_position_bar_button_text {
      flex-direction: column;
      padding-top: 15px;
      gap: 16px;
  }

  a.G_O_banner_section_position_button_by {
      font-size: 16px;
      padding: 13px 26px;
  }

  .G_O_banner_section_position_text {
      font-size: 16px;
  }

  .G_O_banner_section_position_text_btm_images {
      margin: 0 auto;
      width: 283px;
      height: 250px;
  }

  .G_O_banner_section_position_text_btm_img {
      object-position: center;
      padding-bottom: 0 !important;
  }
  }
  @media screen and (max-width: 1440px) {
      .G_O_banner_section_images_box {
          height: 100%;
      }
  }
  @media screen and (max-width: 1150px) {
      .G_O_banner_section_position_bar {
      padding: 100px 10px 0 16px;
  }

  .G_O_banner_section_position_bar_content{
      max-width: 600px;
  }

  .G_O_banner_section_position_bar_Heading {
      font-size: 34px;
  }

  .G_O_banner_section_position_text_btm_images {
      width: 290px;
      height: 290px;
  }
  }
</style>
<section class="G_O_banner_section_Golime_new_banner">
  <div class="G_O_banner_section_innr">
    <div class="G_O_banner_section_images_box">
      {% if section.settings.banner_golime_img_desktop_img != blank %}
        <img
          src="{{ section.settings.banner_golime_img_desktop_img | img_url:'master' }}"
          alt="{{ section.settings.banner_golime_img_desktop_img.alt | escape  }}"
          class="G_O_banner_section_img_desktop"
        >
      {% endif %}
      {% if section.settings.banner_golime_img_mobile_img != blank %}
        <img
          src="{{ section.settings.banner_golime_img_mobile_img | img_url:'master' }}"
          alt="{{ section.settings.banner_golime_img_mobile_img.alt | escape  }}"
          class="G_O_banner_section_img_mobile"
        >
      {% endif %}
    </div>
    <div class="G_O_banner_section_position_bar">
      <div class="G_O_banner_section_position_bar_container">
        <div class="G_O_banner_section_position_bar_content">
          <h1 class="G_O_banner_section_position_bar_Heading">
            {{ section.settings.main_golime_banner_title_heading_new }}
          </h1>
          <div class="G_O_banner_section_position_bar_Para">
            <p>
              {{ section.settings.golime_new_Banner_section_position_bar_Description_Para }}
            </p>
          </div>
          <div class="G_O_banner_section_position_bar_button_text">
            <a href="#" class="G_O_banner_section_position_button_by">
              <span class="G_O_banner_section_position_button">
                {{- section.settings.main_golime_banner_new_section_position_button -}}
              </span>
              <i class="fa-solid fa-chevron-right"></i>
            </a>
            <a href="#" class="G_O_banner_section_position_Text">
              <span class="G_O_banner_section_position_text">
                {{- section.settings.golime_banner_new_section_position_text_span -}}
              </span>
              <i class="fa-solid fa-chevron-right"></i>
            </a>
          </div>
          <div class="G_O_banner_section_position_text_btm_images_content">
            <div class="G_O_banner_section_position_text_btm_images">
              {% if section.settings.main_golime_new_banner_section_position_text_btm_images_sub != blank %}
                <img
                  src="{{ section.settings.main_golime_new_banner_section_position_text_btm_images_sub |img_url:'master' }}"
                  alt="{{ section.settings.main_golime_new_banner_section_position_text_btm_images_sub.alt | escape  }}"
                  class="G_O_banner_section_position_text_btm_img"
                >
              {% endif %}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
{% schema %}
{
  "name": "Golime Banner New2",
  "settings": [
    {
    "type": "image_picker",
    "id": "banner_golime_img_desktop_img",
    "label": "Img Desktop"
    },
    {
    "type": "image_picker",
    "id": "banner_golime_img_mobile_img",
    "label": "Img Mobile"
    },
    {
    "type": "textarea",
    "id": "main_golime_banner_title_heading_new",
    "label": "Banner Top Heading"
    },
    {
    "type": "richtext",
    "id": "golime_new_Banner_section_position_bar_Description_Para",
    "label": "Banner Top Para"
    },
    {
    "type": "text",
    "id": "main_golime_banner_new_section_position_button",
    "label": "Top Banner Button"
    },
    {
    "type": "text",
    "id": "golime_banner_new_section_position_text_span",
    "label": "Top Banner Text"
    },
    {
    "type": "image_picker",
    "id": "main_golime_new_banner_section_position_text_btm_images_sub",
    "label": "Btm Images"
    }
  ],
  "presets": [
    {
    "name": "Golime Banner New2"
    }
  ]
}
{% endschema %}
